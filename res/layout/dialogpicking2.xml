                android:layout_height="wrap_content"
                android:text="Jumlah"
                android:id="@+id/TextView" android:layout_gravity="center"
                android:textColor="@android:color/black" android:textStyle="bold" android:textSize="14sp"
                android:layout_marginLeft="5sp" android:gravity="right"/>

    </LinearLayout>


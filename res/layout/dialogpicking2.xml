    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                  android:orientation="horizontal"
                  android:layout_width="match_parent"
                  android:layout_height="50dip" android:background="#F0ECEB" android:gravity="center_vertical|center_horizontal">
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Batal"
                android:id="@+id/OPTPPCKDialog_BtnBatal" android:layout_gravity="center" style="@style/btnStyleRed"
                android:layout_marginRight="10dip"/>
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Submit"
                android:id="@+id/OPTPPCKDialog_BtnSubmit" android:layout_gravity="center" style="@style/btnStyleGreen"

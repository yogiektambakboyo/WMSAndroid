    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                  android:orientation="horizontal"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content" android:background="#FDB813"
                  android:id="@+id/OPTPPCKDialog_Status"
            >
        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Tidak Ada Stok"
                android:id="@+id/TextView" android:layout_gravity="center"
                android:textColor="@android:color/black" android:textSize="14sp"
                android:layout_marginLeft="5sp" android:gravity="center_vertical|center_horizontal"
                android:textStyle="bold"/>

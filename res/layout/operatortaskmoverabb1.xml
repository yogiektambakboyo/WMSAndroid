<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent" android:background="@android:color/white">
    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Daftar Semua Barang Berlebih"
            android:id="@+id/textView" android:layout_gravity="center" android:textSize="20sp"
            android:textIsSelectable="false" android:textColor="@android:color/black" android:textStyle="bold"
            android:layout_marginBottom="5dip" android:gravity="center_vertical|center_horizontal"
            android:autoText="false" android:background="@android:color/darker_gray"/>

    <ListView
            android:id="@android:id/list"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"/>

</LinearLayout>